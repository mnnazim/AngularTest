import { ViewportRuler } from '@angular/cdk/scrolling';
import { AfterContentInit, ElementRef, NgZone, OnDestroy, ChangeDetectorRef } from '@angular/core';
export declare class NgxNavbarDynamicExpandDirective implements AfterContentInit, OnDestroy {
    private _viewportRuler;
    private _elRef;
    private _ngZone;
    private _cdRef;
    private _isExpanded;
    readonly isExpanded: boolean;
    private _onDestroy;
    private _update;
    private isExpanded$;
    constructor(_viewportRuler: ViewportRuler, _elRef: ElementRef<HTMLElement>, _ngZone: NgZone, _cdRef: ChangeDetectorRef);
    ngAfterContentInit(): void;
    ngOnDestroy(): void;
    updateExpansion(): void;
}
